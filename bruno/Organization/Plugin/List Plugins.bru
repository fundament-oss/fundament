meta {
  name: List Plugins
  type: http
  seq: 1
}

post {
  url: {{organizationUrl}}/organization.v1.PluginService/ListPlugins
  body: json
  auth: inherit
}

headers {
  Content-Type: application/json
}

body:json {
  {}
}

script:post-response {
  if (res.headers['content-type']?.includes('application/json') && res.body.plugins?.length > 0) {
    bru.setVar("pluginId", res.body.plugins[0].id);
  }
}

settings {
  encodeUrl: true
  timeout: 0
}
